---
title: Fun Page
---

<!DOCTYPE html>
<html lang="{{ locale }}" dir="{{ dir }}">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>{{ metaTitle ~ " - " if metaTitle }}{{ site.title }}</title>
  <meta name="description" content="{{ metaDescription if metaDescription else site.description }}">
  <meta name="keywords" content="{{ site.keywords }}" />
  <!-- Open Graph + Twitter -->
  <meta name="twitter:card" content="summary">
  <meta name="twitter:creator" content="{{ site.authorTwitter }}">
  <meta property="og:url" content="{{ site.url }}">
  <meta property="og:title" content="{{ metaTitle ~ ' - ' if metaTitle }}{{ site.title }}">
  <meta property="og:description" content="{{ metaDescription if metaDescription else site.description }}">
  <meta property="og:image" content="{{ metaImage if metaImage else site.metaImage }}">

  <!-- Apple icon (minimal) -->
  <link rel="shortcut icon" href="/static/images/favicon.ico">
  <!-- CSS -->
    <link rel="stylesheet" href="/static/css/main.css?ref={{ site.buildDate | date('X') }}">
</head>

<body data-barba="wrapper">

<!-- ======= Header ======= -->
<header id="header" class="header fixed-top">
  <div class="container-fluid container-xl d-flex align-items-start justify-content-between">
    <div class="logos">

      <a href="index.html" class="logorf d-flex align-items-center">
        <img src="/static/images/logorf.png" alt="">
      </a>
      <a href="index.html" class="logo d-flex align-items-center">
        <img src="/static/images/logo.png" alt="">
      </a>
    </div>
    {% include "partials/nav.njk" %}


  </div>
</header><!-- End Header -->


{% block hero %}

{% endblock %}



  {% block banimg %}
  {% endblock %}


  {% block contenthere %}
    {{ content | safe }}
  {% endblock %}


  <!-- ======= Footer ======= -->
{% include "partials/footer.njk" %}


  <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

</body>

<script>
  function openNav() {
    document.getElementById("mySidebar").style.width = "250px";
    document.getElementById("pageCont").style.marginLeft = "250px";
  }

  function closeNav() {
    document.getElementById("mySidebar").style.width = "0";
    document.getElementById("pageCont").style.marginLeft= "0";
  }
</script>

<script type="module" src="/static/scripts/index.js?ref={{ site.buildDate | date('X') }}"></script>

</html>
