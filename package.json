{
  "name": "11ty-bootsrap-snowpack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "snowpack build",
    "dev": "snowpack dev  --config snowpack.config.js",
    "watch": "yarn watch:js  & yarn watch:css ",
    "compile:js": "yarn run esbuild --bundle --define:process.env.NODE_ENV=\\\"development\\\" --minify --sourcemap src/scripts/index.js --outdir=./_build/js",
    "watch:js": "nodemon --watch ./src/scripts --exec \"yarn compile:js\" -e ts,js,jsx,scss,png,jpg,svg",
    "compile:css": "yarn run sass --style=compressed --load-path=node_modules  src/css:./_build/styles ",
    "watch:css": "yarn run sass -c --poll --watch --load-path=node_modules  src/css:./_build/styles "
  },
  "keywords": [],
  "author": "",
  "devDependencies": {
    "cross-env": "^7.0.2",
    "lodash.get": "^4.4.2",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "snowpack": "^3.8.8",
    "snowpack-plugin-sass": "^1.0.2",
    "templite": "^1.1.0"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@glidejs/glide": "^3.4.1",
    "@popperjs/core": "^2.9.3",
    "@snowpack/plugin-run-script": "^2.3.0",
    "@snowpack/plugin-sass": "1.3.0",
    "@splidejs/splide": "^2.4.21",
    "@splidejs/splide-extension-grid": "^0.3.1",
    "@splidejs/splide-extension-video": "^0.4.6",
    "@srexi/purecounterjs": "^1.2.1",
    "aos": "^3.0.0-beta.6",
    "bootstrap": "^5.1.0",
    "bootstrap-icons": "^1.5.0",
    "eleventy-plugin-i18n": "^0.1.3",
    "eleventy-plugin-reading-time": "^0.0.1",
    "file-loader": "^6.2.0",
    "glightbox": "^3.1.0",
    "gsap": "^3.6.0",
    "imagesloaded": "^4.1.4",
    "jquery": "^3.6.0",
    "luxon": "^1.26.0",
    "moment": "^2.26.0",
    "prismic-dom": "^2.2.5",
    "raw-loader": "^4.0.2",
    "sass": "1.32",
    "shufflejs": "^5.4.1",
    "snowpack-plugin-raw-file-loader": "^1.0.9",
    "splitting": "^1.0.6",
    "swiper": "7.2.0",
    "tiny-emitter": "^2.1.0"
  },
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "src"
    ]
  }
}
